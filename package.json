{
  "name": "GTAH",
  "version": "0.1",
  "repository": "git@github.com:karabe/GTAH.git",
  "author": "karabe <karabe000@gmail.com>",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.0.1",
    "vue-jest": "^2.6.0",
    "web-ext": "^2.6.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4"
  },
  "scripts": {
    "start": "web-ext run --source-dir ./addon/",
    "test": "jest",
    "watch": "webpack --watch",
    "prod": "webpack --mode production"
  },
  "dependencies": {
    "uuid": "^3.2.1",
